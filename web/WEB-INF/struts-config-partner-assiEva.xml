	<action
      path="/assiEvaTrees"
      type="com.boco.eoms.partner.assiEva.webapp.action.AssiEvaTreeAction"
      scope="request"
      name="assiEvaTreeForm"
      input="mainMenu"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="assiEvaKpiTree"
        path="/WEB-INF/pages/partner/assiEva/assiEvaTree/assiEvaKpiTree.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="newNode"
        path="/WEB-INF/pages/partner/assiEva/assiEvaTree/assiEvaTreeForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="editNode"
        path="/WEB-INF/pages/partner/assiEva/assiEvaTree/assiEvaTreeForm.jsp"
        redirect="false"
        contextRelative="true"
      />
