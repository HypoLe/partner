	<!-- ========== Global Exceptions Definitions =================================== -->
	<global-exceptions>
		<!-- 
			<exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
		-->
	</global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward name="mainMenu" path="/mainMenu.do" redirect="true" contextRelative="true"/>
		<forward
          name="success"
          path="/common/success.jsp"
          redirect="false"
          contextRelative="true"
      	/>
      	<forward
          name="fail"
          path="/common/fail.jsp"
          redirect="false"
          contextRelative="true"
      	/>
	</global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->


	<action-mappings>
	
	<action
      path="/chanEvaTrees"
      type="com.boco.eoms.partner.chanEva.webapp.action.ChanEvaTreeAction"
      scope="request"
      name="chanEvaTreeForm"
      input="mainMenu"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="chanEvaKpiTree"
        path="/WEB-INF/pages/partner/chanEva/chanEvaTree/chanEvaKpiTree.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="newNode"
        path="/WEB-INF/pages/partner/chanEva/chanEvaTree/chanEvaTreeForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="editNode"
        path="/WEB-INF/pages/partner/chanEva/chanEvaTree/chanEvaTreeForm.jsp"
        redirect="false"
        contextRelative="true"
      />
