<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans> 
		<form-bean
		  name="pnrAgreementMainForm"
		  type="com.boco.eoms.partner.agreement.webapp.form.PnrAgreementMainForm"
		/>
		<form-bean
		  name="pnrAgreementWorkForm"
		  type="com.boco.eoms.partner.agreement.webapp.form.PnrAgreementWorkForm"
		/>
		<form-bean
		  name="pnrAgreementEvaForm"
		  type="com.boco.eoms.partner.agreement.webapp.form.PnrAgreementEvaForm"
		/>
		<form-bean
		  name="pnrAgreementAuditForm"
		  type="com.boco.eoms.partner.agreement.webapp.form.PnrAgreementAuditForm"
		/>		
		<form-bean
		  name="pnrFeeInfoPayForm"
		  type="com.boco.eoms.partner.feeInfo.webapp.form.PnrFeeInfoPayForm"
		/>
	</form-beans>
	<!-- ========== Global Exceptions Definitions =================================== -->
    <global-exceptions>
        <exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
    </global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward
		  name="success"
		  path="/common/success.jsp"
		  redirect="true"
		  contextRelative="true"
		/>
		<forward
		  name="nopriv"
		  path="/common/nopriv.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="fail"
		  path="/common/failure.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="refreshParent"
		  path="/WEB-INF/pages/partner/agreement/refreshParent.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
    </global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->

	<action-mappings>
		<action
		  path="/pnrAgreementMains"
		  type="com.boco.eoms.partner.agreement.webapp.action.PnrAgreementMainAction"
		  name="pnrAgreementMainForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementMainForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			
			<forward
			  name="statistics"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementStatistics.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			
			<forward
			  name="unAuditList"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementUnAuditList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="audit"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementAudit.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="detail"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementMainDetail.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementMainList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			 <forward
			  name="queryContentsList"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementContentsList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			 <forward
			  name="queryContentsListAgr"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementContentsListAgr.jsp"
			  redirect="false"
			  contextRelative="true"
			/>		
			<forward
			  name="searchPnraw"
			  path="/partner/agreement/pnrAgreementMains.do"
			  redirect="true"
			  contextRelative="true"
			/>		
			<forward
			  name="editAgreement"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementOverFrom.jsp"
			  redirect="false"
			  contextRelative="true"
			/>	
			<forward
			  name="closeAgreement"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementOverFrom.jsp"
			  redirect="false"
			  contextRelative="true"
			/>				
			<forward
			  name="unAuditClosedList"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementUnAuditClosedList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>				
			<forward
			  name="createWork"
			  path="/WEB-INF/pages/partner/agreement/createWork.jsp"
			  redirect="false"
			  contextRelative="true"
			/>	
			<forward
			  name="createEva"
			  path="/WEB-INF/pages/partner/agreement/createEva.jsp"
			  redirect="false"
			  contextRelative="true"
			/>	
			<forward
			  name="uploadSumUp"
			  path="/WEB-INF/pages/partner/agreement/pnrUploadSumUp.jsp"
			  redirect="false"
			  contextRelative="true"
			/>	
			<forward
			  name="rejectWorkSummary"
			  path="/WEB-INF/pages/partner/agreement/rejectWorkSummary.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="haveNoRight"
			  path="/WEB-INF/pages/partner/agreement/haveNoRight.jsp"
			  redirect="false"
			  contextRelative="true"/>	
			<forward
			  name="auditAgreementForCreate"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementAudit.jsp"
			  redirect="false"
			  contextRelative="true"/>
			<forward
			  name="auditForClose"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementOverFrom.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
		<action
		  path="/pnrAgreementWorks"
		  type="com.boco.eoms.partner.agreement.webapp.action.PnrAgreementWorkAction"
		  name="pnrAgreementWorkForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementWorkForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementWorkList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
		<action
		  path="/pnrAgreementEvas"
		  type="com.boco.eoms.partner.agreement.webapp.action.PnrAgreementEvaAction"
		  name="pnrAgreementEvaForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementEvaForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementEvaList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
		<action
		  path="/pnrAgreementAudits"
		  type="com.boco.eoms.partner.agreement.webapp.action.PnrAgreementAuditAction"
		  name="pnrAgreementAuditForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementOverFrom.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="auditForCreate"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementAudit.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="auditForClose"
			  path="/WEB-INF/pages/partner/agreement/pnrAgreementCloseAudit.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
		
	</action-mappings>
 	<controller nocache="true" inputForward="true" maxFileSize="2M" />
	<message-resources parameter="com/boco/eoms/partner/agreement/config/applicationResources-partner-agreement"/>
</struts-config>


