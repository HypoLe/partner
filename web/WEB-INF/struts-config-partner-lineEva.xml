<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
	  <form-bean
        name="lineEvaTreeForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaTreeForm"
      />
      <form-bean
        name="lineEvaKpiForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaKpiForm"
      />
      <form-bean
        name="lineEvaTemplateForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaTemplateForm"
      />
      <form-bean
        name="lineEvaTaskForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaTaskForm"
      />
      <form-bean
        name="lineEvaKpiInstanceForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaKpiInstanceForm"
      />
      <form-bean
        name="lineEvaAuditForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaAuditForm"
      />
      <form-bean
        name="lineEvaKpiInstanceForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaKpiInstanceForm"
      />
      <form-bean
        name="lineEvaReportMultiDeptForm"
        type="com.boco.eoms.partner.lineEva.webapp.form.LineEvaReportMultiDeptForm"
      />
	</form-beans>

	<!-- ========== Global Exceptions Definitions =================================== -->
	<global-exceptions>
		<!-- 
			<exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
		-->
	</global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward name="mainMenu" path="/mainMenu.do" redirect="true" contextRelative="true"/>
		<forward
          name="success"
          path="/common/success.jsp"
          redirect="false"
          contextRelative="true"
      	/>
      	<forward
          name="fail"
          path="/common/fail.jsp"
          redirect="false"
          contextRelative="true"
      	/>
	</global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->


	<action-mappings>
	
	<action
      path="/lineEvaTrees"
      type="com.boco.eoms.partner.lineEva.webapp.action.LineEvaTreeAction"
      scope="request"
      name="lineEvaTreeForm"
      input="mainMenu"
      parameter="method"
      unknown="false"
      validate="false"
    >
      <set-property
        property="cancellable"
        value="true"
      />
      <forward
        name="lineEvaKpiTree"
        path="/WEB-INF/pages/partner/lineEva/lineEvaTree/lineEvaKpiTree.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="newNode"
        path="/WEB-INF/pages/partner/lineEva/lineEvaTree/lineEvaTreeForm.jsp"
        redirect="false"
        contextRelative="true"
      />
      <forward
        name="editNode"
        path="/WEB-INF/pages/partner/lineEva/lineEvaTree/lineEvaTreeForm.jsp"
        redirect="false"
        contextRelative="true"
      />
    </action>
    
    <action
