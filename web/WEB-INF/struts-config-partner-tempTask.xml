<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans> 
		<form-bean
		  name="pnrTempTaskMainForm"
		  type="com.boco.eoms.partner.tempTask.webapp.form.PnrTempTaskMainForm"
		/>
		<form-bean
		  name="pnrTempTaskWorkForm"
		  type="com.boco.eoms.partner.tempTask.webapp.form.PnrTempTaskWorkForm"
		/>
		<form-bean
		  name="pnrTempTaskEvaForm"
		  type="com.boco.eoms.partner.tempTask.webapp.form.PnrTempTaskEvaForm"
		/>
	</form-beans>
	<!-- ========== Global Exceptions Definitions =================================== -->
    <global-exceptions>
        <exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
    </global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward
		  name="success"
		  path="/common/success.jsp"
		  redirect="true"
		  contextRelative="true"
		/>
		<forward
		  name="nopriv"
		  path="/common/nopriv.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="fail"
		  path="/common/failure.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="refreshParent"
		  path="/WEB-INF/pages/partner/tempTask/refreshParent.jsp"
		  redirect="false"
		  contextRelative="true"
		/>	
    </global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->

	<action-mappings>
		<action
		  path="/pnrTempTaskMains"
		  type="com.boco.eoms.partner.tempTask.webapp.action.PnrTempTaskMainAction"
		  name="pnrTempTaskMainForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskMainForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="unAuditList"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskUnAuditList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="audit"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskAudit.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="detail"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskMainDetail.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskMainList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="searchPnrttw"
			  path="/partner/tempTask/pnrTempTaskMains.do"
			  redirect="true"
			  contextRelative="true"
			/>
			<forward
			  name="createWork"
			  path="/WEB-INF/pages/partner/tempTask/createWork.jsp"
			  redirect="false"
			  contextRelative="true"
			/>			
		</action>
		<action
		  path="/pnrTempTaskWorks"
		  type="com.boco.eoms.partner.tempTask.webapp.action.PnrTempTaskWorkAction"
		  name="pnrTempTaskWorkForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskWorkForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskWorkList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="undoList"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskWorkUndoList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="workExecute"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskExecute.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="flushParentPage"
			  path="/WEB-INF/pages/partner/workplan/pnrWorkRefresh.jsp"
			  redirect="false"
			  contextRelative="true"
			/>				
		</action>
		<action
		  path="/pnrTempTaskEvas"
		  type="com.boco.eoms.partner.tempTask.webapp.action.PnrTempTaskEvaAction"
		  name="pnrTempTaskEvaForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskEvaForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/tempTask/pnrTempTaskEvaList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
	</action-mappings>
 	<controller nocache="true" inputForward="true" maxFileSize="2M" />
	<message-resources parameter="com/boco/eoms/partner/tempTask/config/applicationResources-partner-tempTask"/>
</struts-config>


