<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config> 
	<form-beans> 
		<form-bean
		  name="pnrFeeInfoMainForm"
		  type="com.boco.eoms.partner.feeInfo.webapp.form.PnrFeeInfoMainForm"
		/>
		<form-bean
		  name="pnrFeeInfoPlanForm"
		  type="com.boco.eoms.partner.feeInfo.webapp.form.PnrFeeInfoPlanForm"
		/>
		<form-bean
		  name="pnrFeeInfoPayForm"
		  type="com.boco.eoms.partner.feeInfo.webapp.form.PnrFeeInfoPayForm"
		/>
	</form-beans>
	<!-- ========== Global Exceptions Definitions =================================== -->
    <global-exceptions>
        <exception type="java.lang.Exception" key="errors.general"
			handler="com.boco.eoms.base.webapp.action.ActionExceptionHandler" />
    </global-exceptions>

	<!-- ========== Global Forward Definitions =================================== -->
	<global-forwards>
		<forward
		  name="success"
		  path="/common/success.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="nopriv"
		  path="/common/nopriv.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="fail"
		  path="/common/failure.jsp"
		  redirect="false"
		  contextRelative="true"
		/>
		<forward
		  name="refreshParent"
		  path="/WEB-INF/pages/partner/feeInfo/refreshParent.jsp"
		  redirect="false"
		  contextRelative="true"
		/>		
    </global-forwards>

	<!-- ========== Action Mapping Definitions =================================== -->

	<action-mappings>
		<action
		  path="/pnrFeeInfoMains"
		  type="com.boco.eoms.partner.feeInfo.webapp.action.PnrFeeInfoMainAction"
		  name="pnrFeeInfoMainForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoMainForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="addProxyContract"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoAddProxyContract.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="deptTree"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoTree.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="unAuditInfoList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoUnAuditList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="unAuditPayList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeePayUnAuditList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="audit"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoAudit.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="detail"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoMainDetail.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="checkOneRecord"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPayOneRecord.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="history"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoMainHistory.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoMainList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="flushPage"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoFresh.jsp"
			  redirect="false"
			  contextRelative="true"
			/>	
			<forward
			  name="searchPnrFeeInfo"
			  path="/partner/feeInfo/pnrFeeInfoMains.do"
			  redirect="true"
			  contextRelative="true"
			/>	
			<forward
			  name="returnValue"
			  path="/WEB-INF/pages/partner/agreement/returnValue.jsp"
			  redirect="false"
			  contextRelative="true"
			/>	
			<forward
			  name="feeInfoFlowList"
			  path="/WEB-INF/pages/partner/feeInfo/feeInfoFlowList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="contractRelateProtocol"
			  path="/WEB-INF/pages/partner/feeInfo/pnrContractRelateProtocol.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="editProxy"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoEditProxy.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
		<action
		  path="/pnrFeeInfoPlans"
		  type="com.boco.eoms.partner.feeInfo.webapp.action.PnrFeeInfoPlanAction"
		  name="pnrFeeInfoPlanForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="edit"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanForm.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="list"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="undoList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanUndoList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="pnrFeeInfoPlanAllCompleteList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanAllCompleteList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="pnrFeeInfoPlanConfirmList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanConfirmList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="pnrFeeInfoPlanB2UploadList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanB2UploadList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="pnrFeeInfoPlanAuditBUploadList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanAuditBUploadList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="pnrFeeInfoPlanPassBUploadlist"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanPassBUploadlist.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="planPay"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoExecute.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action><action
		  path="/pnrFeeInfoPays"
		  type="com.boco.eoms.partner.feeInfo.webapp.action.PnrFeeInfoPayAction"
		  name="pnrFeeInfoPayForm"
		  scope="request"
		  input="mainMenu"
		  parameter="method"
		  unknown="false"
		  validate="false"
		>
			<set-property
			  property="cancellable"
			  value="true"
			/>
			<forward
			  name="undoList"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPlanUndoList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="planPay"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPay.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="planPayReply"
			  path="/WEB-INF/pages/partner/feeInfo/pnrFeeInfoPayReply.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="haveNoRight"
			  path="/WEB-INF/pages/partner/feeInfo/haveNoRight.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
			<forward
			  name="feepayUndoList"
			  path="/WEB-INF/pages/partner/feeInfo/feepayUndoList.jsp"
			  redirect="false"
			  contextRelative="true"
			/>
		</action>
	</action-mappings>
 	<controller nocache="true" inputForward="true" maxFileSize="2M" />
	<message-resources parameter="com/boco/eoms/partner/feeInfo/config/applicationResources-partner-feeInfo"/>
</struts-config>


