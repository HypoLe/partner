<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.boco.eoms.materials.model.StoreInventory"
            table="mate_store_inventory">

    	<id name="id" column="id" unsaved-value="null" >
    		<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    		<generator class="uuid.hex" />
    	</id>

        <property name="storeSid" column="store_sid"  />
        <property name="storeSname" column="store_sname"  />
        <property name="materialName" column="material_name"  />
        <property name="materialPrice" column="material_price"  />
        <property name="onhand"   />
        <property name="materialEncode" column="material_encode"  />
        <property name="materialSpecification" column="material_specification"  />
        <property name="materialCategoryNum" column="material_category_num"  />
        <property name="materialCategoryDetail" column="material_category_detail"  />
        <property name="materialManufacturer" column="material_manufacturer"  />
        <property name="materialUnit" column="material_unit"  />
        <property name="materialId" column="material_id"  />
        <property name="totalAmount" column="total_amount"  />
        <property name="remark" column="remark"  />
      
        

    </class>    

</hibernate-mapping>

