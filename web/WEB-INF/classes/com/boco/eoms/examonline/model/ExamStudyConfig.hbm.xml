<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.boco.eoms.examonline.model.ExamStudyConfig"  table="ExamStudyConfig">
<id column="ID" name="id" type="string">
<generator class="uuid.hex"/>
</id>
<property column="SEL_VALUE" length="255" name="selValue" not-null="true" type="string"/>
<property column="SEL_NAME" length="1000" name="selName" not-null="true" type="string"/>
<property column="USER_ID" length="30" name="userId" not-null="true" type="string"/>
</class>
</hibernate-mapping>
