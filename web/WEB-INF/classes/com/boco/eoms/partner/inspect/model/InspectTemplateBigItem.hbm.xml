<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.boco.eoms.partner.inspect.model.InspectTemplateBigItem" table="pnr_inspect_template_bigitem">
	            
    <id name="id" column="id" unsaved-value="null">
	    <generator class="uuid.hex">
	    </generator>
    </id>
    				            
    <property name="templateId"
            column="template_id"
            not-null="false"
            unique="false">
    </property>
    		            
    		            
    <property name="name"
            column="name"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="orderNo"
            column="order_no"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="itemNum"
            column="item_num"
            not-null="false"
            unique="false">
    </property>
    
    <property name="status"
            column="status"
            not-null="false"
            unique="false">
    </property>
</class>    
</hibernate-mapping>

