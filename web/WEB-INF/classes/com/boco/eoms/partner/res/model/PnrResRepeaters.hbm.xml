<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.boco.eoms.partner.res.model.PnrResRepeaters" table="PNR_RES_REPEATERS">
	            
    <id name="id" column="id" unsaved-value="null">

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>
    </id>
     <property name="address"
            column="address"
            not-null="false"
            unique="false">
    </property>
    
    <property name="adjustmentMessage"
            column="adjustment_message"
            not-null="false"
            unique="false">
    </property>
    
    <property name="netType"
            column="net_type"
            not-null="false"
            unique="false">
    </property>
    
    <property name="openTime"
            column="g2_open_time"
            not-null="false"
            unique="false">
    </property>
    
    <property name="propertyType"
            column="property_type"
            not-null="false"
            unique="false">
    </property>
    
    <property name="sharing"
            column="sharing"
            not-null="false"
            unique="false">
    </property>
    
    <property name="TDOpenTime"
            column="td_open_time"
            not-null="false"
            unique="false">
    </property>
   
   
</class>    
</hibernate-mapping>

