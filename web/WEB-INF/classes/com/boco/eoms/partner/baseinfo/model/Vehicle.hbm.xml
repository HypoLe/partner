<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.baseinfo.model.Vehicle"
            table="pnr_base_vehicle"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="region"
            	column="region"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="city"
            	column="city"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="provider"
            	column="provider"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="serviceProfession"
            	column="service_profession"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="use"
            	column="use"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="disposeNo"
            	column="dispose_no"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isDel"
            	column="isdel"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="userNameDel"
            	column="username_del"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="timeDel"
            	column="time_del"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        <property
            name="partnerid"
            	column="partnerid"
                length="32"
                not-null="false"
                unique="false"
        >
        </property>    
        <property
            name="bigpartnerid"
            	column="bigpartnerid"
                length="32"
                not-null="false"
                unique="false"
        >
        </property>  
        
    </class>    

</hibernate-mapping>

