<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.trans.IrmsTransOdf" table="irms_trans_odf">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="odfName" column="odf_name"/>
				<property name="odfNo" column="odf_no"/>
				<property name="relatedDistrict" column="related_district"/>
				<property name="status" column="status"/>
				<property name="vendor" column="vendor"/>
				<property name="position" column="position"/>
				<property name="longitude" column="longitude"/>
				<property name="latitude" column="latitude"/>
				<property name="type" column="type"/>
				<property name="faceNum" column="face_num"/>
				<property name="eachFaceRowNum" column="each_face_row_num"/>
				<property name="eachFaceColNum" column="each_face_col_num"/>
				<property name="property" column="property"/>
				<property name="use" column="use"/>
				<property name="useUnit" column="use_unit"/>
				<property name="owner" column="owner"/>
				<property name="capacity" column="capacity"/>
				<property name="remark" column="remark"/>
				<property name="createTime" column="create_time"/>
				<property name="relatedDistirctId" column="related_distirct_id"/>
    </class>    
</hibernate-mapping>