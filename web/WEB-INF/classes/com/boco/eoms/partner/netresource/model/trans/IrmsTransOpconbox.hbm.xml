<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.trans.IrmsTransOpconbox" table="irms_trans_opconbox">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="boxName" column="box_name"/>
				<property name="boxType" column="box_type"/>
				<property name="relatedDistrict" column="related_district"/>
				<property name="boxObject" column="box_object"/>
				<property name="vendor" column="vendor"/>
				<property name="status" column="status"/>
				<property name="connectWay" column="connect_way"/>
				<property name="capacity" column="capacity"/>
				<property name="remark" column="remark"/>
				<property name="createTime" column="create_time"/>
				<property name="relatedDistirctId" column="related_distirct_id"/>
				<property name="boxObjectId" column="box_object_id"/>
    </class>    
</hibernate-mapping>