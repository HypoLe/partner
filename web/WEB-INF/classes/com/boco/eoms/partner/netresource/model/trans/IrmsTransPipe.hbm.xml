<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.trans.IrmsTransPipe" table="irms_trans_pipe">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="pipeName" column="pipe_name"/>
				<property name="relatedArea" column="related_area"/>
				<property name="status" column="status"/>
				<property name="projectName" column="project_name"/>
				<property name="length" column="length"/>
				<property name="completeTime" column="complete_time"/>
				<property name="remark" column="remark"/>
				<property name="createTime" column="create_time"/>
				<property name="relatedAreaId" column="related_area_id"/>
    </class>    
</hibernate-mapping>