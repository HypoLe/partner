<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.boco.eoms.partner.management.model.Management" table="PNR_COST_MANAGEMENT">
	            
    <id name="id" column="id" unsaved-value="null">

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>
    </id>
    
    <property name="createUser"
            column="createUser"
            not-null="false"
            unique="false">
    </property>
    
    <property name="createTime"
            column="createTime"
            not-null="false"
            unique="false">
    </property>
    
    <property name="isDeleted"
            column="isDeleted"
            not-null="false"
            unique="false">
    </property>
    				            
    <property name="partnerId"
            column="partnerId"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="partnerNum"
            column="partnerNum"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="expenseCost"
            column="expenseCost"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="beginCostTime"
            column="beginCostTime"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="endCostTime"
            column="endCostTime"
            not-null="false"
            unique="false">
    </property>
    		            
    <property name="remark"
            column="remark"
            not-null="false"
            unique="false">
    </property>
    
</class>    
</hibernate-mapping>

