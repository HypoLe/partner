<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
            name="com.boco.eoms.partner.dataSynch.model.SynchExceptionRecord"
            table="irms_datasynch_exception"
    >
	    <id
	        name="id"
	        column="id"
	        unsaved-value="null"
	    >
	    	<generator class="uuid.hex"></generator>
	    </id>
    
        <property name="cuid" column="cuid" /> 
        <property name="dataType" column="data_type" /> 
        <property name="exceptionField" column="exception_field" /> 
        <property name="exceptionReason" column="exception_reason" /> 
        <property name="exceptionFileName" column="exception_file_name" /> 
        <property name="createTime" column="create_time" /> 
    </class>    

</hibernate-mapping>
    