<?xml version="1.0" encoding="UTF-8"?>

<!-- 传输线路-专业 -->
<excel2xml>
	<!-- 人手井-IrmsTransWell -->
	<IrmsTransWell>
		<field name="name"		excelCellNum="1"			remark="人手井名称"/>
		<field name="related_area"		excelCellNum="2"			remark="所属区域"/>
