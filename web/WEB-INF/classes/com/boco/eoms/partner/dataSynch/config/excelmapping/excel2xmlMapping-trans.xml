		<field name="latitude"		excelCellNum="9"			remark="纬度"/>
		<field name="use_unit"		excelCellNum="10"			remark="使用单位"/>
		<field name="owner_unit"		excelCellNum="11"			remark="所有权人"/>
	</IrmsTransWell>
