<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.assess.AssAlgorithm.model.AssSelectedInstance"
            table="ass_selected_instance"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="kpiId"
            	column="kpi_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
	<property
            name="taskId"
            	column="task_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
	<property
            name="partnerId"
            	column="partner_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
	<property
            name="time"
            	column="time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
	<property
            name="reportId"
            	column="report_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
	<property
            name="configId"
            	column="config_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
	<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="weight"
            	column="weight"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="areaId"
            	column="area_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

