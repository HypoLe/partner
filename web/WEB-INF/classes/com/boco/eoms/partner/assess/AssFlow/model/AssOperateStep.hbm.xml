<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.assess.AssFlow.model.AssOperateStep"
            table="ass_operate_step"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>
    
	
	<property
            name="createTime"
            	column="create_time"
                length="32"
                not-null="false"
                unique="false"
        >

    </property>

	<property
            name="reportId"
            	column="report_id"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="operateOrgId"
            	column="operate_org_id"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="operateOrgType"
            	column="operate_org_type"
                length="10"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="operateUser"
            	column="operate_user"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="operateTime"
            	column="operate_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="operateUserContact"
            	column="operate_user_contact"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="operateOpinion"
            	column="operate_opinion"
                length="2000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="state"
            	column="state"
                length="10"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="operateFlag"
            	column="operate_flag"
                length="10"
                not-null="true"
                unique="false"
        >

        </property>


		<property
            name="accessoriesId"
            	column="accessories_id"
                length="50"
                not-null="false"
                unique="false"
        >

        </property>

    </class>    

</hibernate-mapping>

