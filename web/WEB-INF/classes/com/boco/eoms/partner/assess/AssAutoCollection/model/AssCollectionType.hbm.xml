<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.assess.AssAutoCollection.model.AssCollectionType"
            table="ass_collection_type"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="name"
            	column="name"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="serviceAddr"
            	column="service_Addr"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="treeNodeId"
            	column="tree_Node_Id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

        
	<property
            name="userName"
            	column="user_name"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="password"
            	column="password"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>             
    </class>    

</hibernate-mapping>

