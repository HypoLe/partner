                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="remark"
            	column="REMARK"
                length="500"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="addUser"
            	column="add_user"
                length="32"
                not-null="true"
                unique="false"
        >

        </property>

	<property
            name="isDel"
