        >

        </property>

	<property
            name="delTime"
            	column="del_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="updateTime"
            	column="update_time"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="siteNo"
            	column="site_no"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
	<property
            name="idSite"
