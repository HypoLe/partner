<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.boco.eoms.partner.projectCost.model.ProjectCostModel" table="PNR_costmanagement_Projectcost">
		<id name="id" column="id">
   			<generator class="uuid.hex" />
   		</id>
   	<property name="advanceId" />
   	<property name="advanceName" />
   	<property name="money" />
   	<property name="type" />
   	<property name="remark" />
   	<property name="greatUser" />
   	<property name="state" />
   	<property name="isDelete" />
    </class>    
</hibernate-mapping>