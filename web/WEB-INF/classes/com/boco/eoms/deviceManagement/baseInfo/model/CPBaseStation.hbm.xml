<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.boco.eoms.deviceManagement.baseInfo.model.CPBaseStation" table="db_basestation">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>

		<property name="checkPointId" />
		<property name="baseStationLevel" />
		<property name="baseStationName" />
		<property name="baseStationId" />
	</class>
</hibernate-mapping>