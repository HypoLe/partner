<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boco.eoms.deviceManagement.busi.protectline.model.WarningBoardOperate"
		table="dm_protectline_warning"
		>
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		
		<property name="warningBoardId"  />
		<property name="operateTime"  />
		<property name="operateUserId" />
		<property name="operateResult" />
		<property name="operateOpinion" />
		<property name="operateRemark" />
		<property name="operateStatus" />
		<property name="operateTarget" />
		
	</class>
</hibernate-mapping>