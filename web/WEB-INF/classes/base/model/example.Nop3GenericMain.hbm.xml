<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="yourClass" table="yourTable">
		<id name="id" column="id">
   			<generator class="uuid.hex" />
   		</id>
		<!-- Public Area begin WARNING:DO NOT MODIFY,COMMENT BY STEVE -->
		<property name="status" />
		<property name="verticalStatus" />
		<property name="taskOwnerType" />
  		<property name="taskOwner" />
  		<property name="createUserId" />
  		<property name="createDate" />
  		<property name="yearFlag" />
  		<property name="monthFlag" />
  		<property name="dayFlag" />
		<!-- Public Area end:DO NOT MODIFY,COMMENT BY STEVE -->
    </class>    
</hibernate-mapping>