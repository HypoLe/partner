<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.boco.eoms.aggregation.model.Aggregation"
		table="aggregation">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="creatUser" />
		<property name="creatTime" />
		<property name="moduleName" />
		<property name="moduleUrl" />
		<property name="deleted" />
		<property name="content" />
		<property name="accessory" />
		<property name="photo" />
		<property name="remark" />
		<property name="remark2" />
		<property name="remark3" />
		<property name="remark4" />
	</class>
</hibernate-mapping>