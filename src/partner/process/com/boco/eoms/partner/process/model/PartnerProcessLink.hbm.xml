<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.boco.eoms.partner.process.model.PartnerProcessLink" table="pnr_process_link">
	 <id name="id" column="id" unsaved-value="null">
	  	<generator class="uuid.hex"></generator>
     </id>
    <property name="referenceId"  column="reference_id"  not-null="false"  unique="false"></property>
	<property name="state"  column="state"  not-null="false"  unique="false"></property>
	<property name="dataReceiver"  column="data_receiver"  not-null="false"  unique="false"></property>
	<property name="dataSender"  column="data_sender"  not-null="false"  unique="false"></property>
	<property name="createTime"  column="create_time"  not-null="false"  unique="false"></property>
	<property name="reason"  column="reason"  not-null="false"  unique="false"></property>

	</class>
</hibernate-mapping>