<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


	<class name="com.boco.eoms.sheet.base.model.PnrSheetQuery" table="pnr_sheet_query">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="mainId" />
		<property name="provincePnrDept" />
		<property name="cityPnrDept" />
		<property name="countryPnrDept" />
		<property name="groupPnrDept" />
		<property name="sheetType" />
	</class>
</hibernate-mapping>