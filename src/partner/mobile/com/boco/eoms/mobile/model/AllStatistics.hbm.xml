<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boco.eoms.mobile.model.AllStatistics" table="pnr_android_allstatistics">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="statisticsName" />
		<property name="statisticsType"/>
		<property name="currentScore" type="java.lang.Integer"/>
		<property name="previousScore" type="java.lang.Integer"/>
		<property name="addDate"/>
		<property name="addUser"/>
	</class>
</hibernate-mapping>