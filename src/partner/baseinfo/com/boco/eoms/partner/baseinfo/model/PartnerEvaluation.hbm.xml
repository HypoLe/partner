<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.baseinfo.model.PartnerEvaluation"
            table="pnr_eval"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="partnerId"
            	column="partnerId"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="contractId"
            	column="contractId"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="feePlanId"
            	column="feePlanId"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="evaTime"
            	column="evaTime"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="evaResult"
            	column="evaResult"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="evaUser"
            	column="evaUser"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="unContent"
            	column="unContent"
                length="1000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="propose"
            	column="propose"
                length="1000"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="isDel"
            	column="isDel"
                length="8"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="evaDept"
            	column="evaDept"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>


    </class>    

</hibernate-mapping>

