<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.trans.IrmsTransDirbury" table="irms_trans_dirbury">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="dirburyName" column="dirbury_name"/>
				<property name="relatedDistrict" column="related_district"/>
				<property name="status" column="status"/>
				<property name="projectName" column="project_name"/>
				<property name="length" column="length"/>
				<property name="completedDate" column="completed_date"/>
				<property name="remark" column="remark"/>
				<property name="createTime" column="create_time"/>
				<property name="relatedDistirctId" column="related_distirct_id"/>
    </class>    
</hibernate-mapping>