<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.trans.IrmsTransWell" table="irms_trans_well">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="name" column="name"/>
				<property name="relatedArea" column="related_area"/>
				<property name="wellMode" column="well_mode"/>
				<property name="wellType" column="well_type"/>
				<property name="ownership" column="ownership"/>
				<property name="purpose" column="purpose"/>
				<property name="wellRealType" column="well_real_type"/>
				<property name="longitude" column="longitude"/>
				<property name="latitude" column="latitude"/>
				<property name="useUnit" column="use_unit"/>
				<property name="ownerUnit" column="owner_unit"/>
				<property name="remark" column="remark"/>
				<property name="createTime" column="create_time"/>
				<property name="relatedAreaId" column="related_area_id"/>
    </class>    
</hibernate-mapping>