<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.bs.RoomResource" table="pnr_room_resource">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="reNumber" column="re_number"/>
				<property name="city" column="city"/>
				<property name="country" column="country"/>
				<property name="specialty" column="specialty"/>
				<property name="resType" column="res_type"/>
				<property name="resLevel" column="res_level"/>
				<property name="reName" column="re_name"/>
				<property name="address" column="address"/>
				<property name="contact" column="contact"/>
				<property name="contactTel" column="contact_tel"/>
				<property name="longitude" column="longitude"/>
				<property name="latitude" column="latitude"/>
				<property name="source2g" column="source_2g"/>
				<property name="lac" column="lac"/>
				<property name="ci" column="ci"/>
				<property name="bcch" column="bcch"/>
				<property name="source3g" column="source_3g"/>
				<property name="psc" column="psc"/>
				<property name="location" column="location"/>
				<property name="model2g" column="model_2g"/>
				<property name="number2g" column="number_2g"/>
				<property name="model3g" column="model_3g"/>
				<property name="number3g" column="number_3g"/>
    </class>    
</hibernate-mapping>