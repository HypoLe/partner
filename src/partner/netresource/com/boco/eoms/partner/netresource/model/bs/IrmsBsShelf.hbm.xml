<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.bs.IrmsBsShelf" table="irms_bs_shelf">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="labelCn" column="label_cn"/>
				<property name="relatedRackCuid" column="related_rack_cuid"/>
				<property name="sequence" column="sequence"/>
				<property name="slotAmount" column="slot_amount"/>
				<property name="createTime" column="create_time"/>
				<property name="remark" column="remark"/>
				<property name="relatedRackCuidId" column="related_rack_cuid_id"/>
    </class>    
</hibernate-mapping>