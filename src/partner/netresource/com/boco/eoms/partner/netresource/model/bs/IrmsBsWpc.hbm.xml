<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.bs.IrmsBsWpc" table="irms_bs_wpc">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="relatedRoom" column="related_room"/>
				<property name="deviceType" column="device_type"/>
				<property name="deviceSubclass" column="device_subclass"/>
				<property name="labelDev" column="label_dev"/>
				<property name="deviceSequence" column="device_sequence"/>
				<property name="equipmentcode" column="equipmentcode"/>
				<property name="model" column="model"/>
				<property name="relatedVendor" column="related_vendor"/>
				<property name="trademark" column="trademark"/>
				<property name="supplier" column="supplier"/>
				<property name="manageCompany" column="manage_company"/>
				<property name="startTime" column="start_time"/>
				<property name="endTime" column="end_time"/>
				<property name="status" column="status"/>
				<property name="fanCount" column="fan_count"/>
				<property name="expRatingCapacit" column="exp_rating_capacit"/>
				<property name="preserver" column="preserver"/>
				<property name="createTime" column="create_time"/>
				<property name="remark" column="remark"/>
				<property name="relatedRoomId" column="related_room_id"/>
    </class>    
</hibernate-mapping>