				<property name="boundcellType" column="boundcell_type"/>
				<property name="ifRepeator" column="if_repeator"/>
				<property name="isEnegprs" column="is_enegprs"/>
				<property name="coverFiled" column="cover_filed"/>
				<property name="trxCount" column="trx_count"/>
				<property name="openTime" column="open_time"/>
				<property name="remark" column="remark"/>
				<property name="createTime" column="create_time"/>
				<property name="relatedBtssiteCuId" column="related_btssite_cu_id"/>
    </class>    
</hibernate-mapping>