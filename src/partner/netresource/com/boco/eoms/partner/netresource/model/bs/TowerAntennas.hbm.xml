<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.netresource.model.bs.TowerAntennas" table="pnr_tower_antennas">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="anotherTaName" column="another_ta_name"/>
				<property name="taName" column="ta_name"/>
				<property name="city" column="city"/>
				<property name="country" column="country"/>
				<property name="specialty" column="specialty"/>
				<property name="resType" column="res_type"/>
				<property name="resLevel" column="res_level"/>
				<property name="equity" column="equity"/>
				<property name="towerType" column="tower_type"/>
				<property name="antennaType" column="antenna_type"/>
				<property name="towerHeight" column="tower_height"/>
				<property name="longitude" column="longitude"/>
				<property name="latitude" column="latitude"/>
				<property name="rruNumber" column="rru_number"/>
				<property name="repeaterNumber" column="repeater_number"/>
				
    </class>    
</hibernate-mapping>