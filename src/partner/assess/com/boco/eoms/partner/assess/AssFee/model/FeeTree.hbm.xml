<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class        
            name="com.boco.eoms.partner.assess.AssFee.model.FeeTree"
            table="ass_fee_tree"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="price"
            	column="price"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="nodeName"
            	column="NODE_NAME"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="nodeRemark"
            	column="NODE_REMARK"
                length="255"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="unit"
            	column="unit"
                length="64"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="nodeType"
            	column="Node_type"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

		<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="parentNodeId"
            	column="parent_node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="leaf"
            	column="leaf"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

