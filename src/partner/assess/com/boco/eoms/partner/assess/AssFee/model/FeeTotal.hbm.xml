<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.assess.AssFee.model.FeeTotal"
            table="ass_fee_total"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="totalreaMoney"
            	column="totalrea_money"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="totalMoney"
            	column="total_money"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="monthMoney"
            	column="month_money"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="pointMoney"
            	column="point_money"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="quarterMoney"
            	column="quarter_money"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="areaId"
            	column="area_id"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="year"
            	column="year"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="deleted"
            	column="deleted"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

        
    </class>    

</hibernate-mapping>

