<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.assess.AssFee.model.FeeDetail"
            table="ass_fee_detail"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="number"
            	column="numbers"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="price"
            	column="price"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="total"
            	column="total"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="resultId"
            	column="result_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="areaId"
            	column="area_id"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="year"
            	column="year"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="type"
            	column="type"
                length="10"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="deleted"
            	column="deleted"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="unit"
            	column="unit"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

