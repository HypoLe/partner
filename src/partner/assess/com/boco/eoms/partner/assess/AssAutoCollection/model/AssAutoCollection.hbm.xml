<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.boco.eoms.partner.assess.AssAutoCollection.model.AssAutoCollection"
            table="ass_auto_collection"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid.hex">

    </generator>

    </id>

	<property
            name="nodeName"
            	column="node_name"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

	<property
            name="nodeType"
            	column="node_type"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>

		<property
            name="nodeId"
            	column="node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="parentNodeId"
            	column="parent_node_id"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
        <property
            name="leaf"
            	column="leaf"
                length="32"
                not-null="false"
                unique="false"
        >

        </property>
        
    </class>    

</hibernate-mapping>

