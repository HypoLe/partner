<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boco.eoms.partner.inspect.model.SpotcheckTemplateItem" table="pnr_spotcheck_template_item">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="spotcheckTemplateId" column="spotcheck_Template_Id"/>
		<property name="inspectTemplateItemId" column="inspect_Template_Item_Id"/>
	</class>
</hibernate-mapping>