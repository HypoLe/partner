<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boco.eoms.partner.inspect.model.SpotcheckRes" table="pnr_spotcheck_res">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="planId" column="plan_Id"/>
		<property name="planResId" column="plan_Res_Id"/>
		<property name="spotcheckUser" column="spotcheck_User"/>
		<property name="spotcheckTime" column="spotcheck_Time"/>
		<property name="score" column="score"/>
	</class>
</hibernate-mapping>