<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.taskManager.model.CarApproveLink" table="pnr_Car_Approve_Link">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="carApproveId" column="car_Approve_Id"/>
				<property name="operateTime" column="operate_Time"/>
				<property name="operateType" column="operate_Type"/>
				<property name="operateUser" column="operate_User"/>
				<property name="remark" column="remark"/>
    </class>    
</hibernate-mapping>