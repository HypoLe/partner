<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  name="com.boco.eoms.partner.deviceInspect.model.PnrInspectMapping" table="pnr_inspect_mapping">
				<id name="id" column="id" unsaved-value="null">
					<!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
					<generator class="uuid.hex">
					</generator>
				</id>
			    
				<property name="specialty" column="specialty"/>
				<property name="inspectType" column="inspect_type"/>
				<property name="deviceSpecialty" column="device_specialty"/>
				<property name="deviceSpecialtyName" column="device_specialty_name"/>
				<property name="deviceType" column="device_type"/>
				<property name="deviceTypeName" column="device_type_name"/>
				<property name="netresTableName" column="netres_table_name"/>
				<property name="netresFieldName" column="netres_field_name"/>
				<property name="netresFieldValue" column="netres_field_value"/>
				<property name="remark" column="remark"/>
    </class>    
</hibernate-mapping>