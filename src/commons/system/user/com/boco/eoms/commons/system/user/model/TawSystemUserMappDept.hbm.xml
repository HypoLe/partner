<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="com.boco.eoms.commons.system.user.model.TawSystemUserMappDept"
		table="taw_system_user_mappDept">

		<id column="id" name="id" type="string">
			<generator class="uuid.hex" />
		</id>

		<property name="deptId" length="100">

		</property>

		<property name="deptName" length="100">

		</property>

		<property name="localDeptId" length="100">

		</property>

		<property name="localDeptName" length="100">

		</property>
	</class>

</hibernate-mapping>

