<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.boco.eoms.deviceManagement.baseInfo.model.Pole" table="db_pole">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>

		<property name="checkPointId" />
		<property name="poletUser" />
		<property name="loadSysLevel" />
		<property name="loadCableSegmentName" />
		<property name="poleNum" />
		<property name="poleName" />
		<property name="isJointPole" />
		<property name="isCableObligate" />
		<property name="isImportantFocus" />
	</class>
</hibernate-mapping>