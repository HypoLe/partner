<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="com.boco.eoms.deviceManagement.baseInfo.model.AdvertisementPlan"
		table="dm_advertisementplan_info">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="advertisement"/>
		<property name="city"/>
		<property name="createTime"/>
		<property name="createUserId"/>
		<property name="detectStone"/>
		<property name="generalStone"/>
		<property name="remarks"/>
		<property name="reviewer"/>
		<property name="status" />
		
	</class>
</hibernate-mapping>