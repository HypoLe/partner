<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="com.boco.eoms.deviceManagement.baseInfo.model.CheckSegment"
		table="dm_checksegment_info">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="segmentName"/>
		<property name="lineType"/>
		<property name="lineLevel"/>
		<property name="startFlag"/>
		<property name="endFlag"/>
		<property name="segmentType"/>
		<property name="department"/>
		<property name="deleteFlag"/>
	</class>
</hibernate-mapping>