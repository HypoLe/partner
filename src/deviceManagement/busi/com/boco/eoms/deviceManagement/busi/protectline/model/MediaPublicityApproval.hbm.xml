<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.boco.eoms.deviceManagement.busi.protectline.model.MediaPublicityApproval" table="dm_protectline_mediaApproval">
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>

		<property name="approvalUser" />
		<property name="approvalTime" />
		<property name="projectNameID" />
		<property name="approvalContent" />
		<property name="approvalType" />
		<property name="remark" />
		<property name="remark2" />
		<property name="remark3" />
		<property name="remark4" />
	</class>
</hibernate-mapping>